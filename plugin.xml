<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="org.apache.cordova.device" version="0.2.3">
    <name>HealthKit Add Food</name>
    <description>Add food to HealthKit on iOS</description>
    <license>Apache 2.0</license>
    <keywords>cordova,device</keywords>
    <js-module src="www/HKAddFood.js" name="device">
        <clobbers target="device" />
    </js-module>
    <platform name="ios">
        <config-file target="config.xml" parent="/*">
            <feature name="HKAddFood">
                <param name="ios-package" value="CDVAddFood"/>
            </feature>
        </config-file>
        <header-file src="src/ios/CDVAddFood.h" />
        <source-file src="src/ios/CDVAddFood.m" />
    </platform>
</plugin>